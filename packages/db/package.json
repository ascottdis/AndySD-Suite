{
    "name": "@andysd/db",
    "version": "0.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": true,
    "scripts": {
        "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
        "dev": "echo [@andysd/db] dev watcher disabled for now && exit 0",
        "lint": "eslint \"src/**/*.ts\"",
        "typecheck": "tsc --noEmit",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev"
    },
    "dependencies": {
        "@andysd/ui": "workspace:^"
    },
    "devDependencies": {
        "@prisma/client": "^6.19.2",
        "eslint": "latest",
        "prisma": "^6.19.2",
        "tsup": "latest",
        "tsx": "latest",
        "typescript": "latest"
    }
}
